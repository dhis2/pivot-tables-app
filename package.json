{
  "name": "pivot-app",
  "version": "32.0.2",
  "description": "Pivot tables app",
  "main": "index.html",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js src `git diff --cached --name-only | grep \\.js$`",
    "validate": "npm ls --depth 0",
    "prestart": "d2-manifest package.json manifest.webapp",
    "start": "webpack-dev-server",
    "build": "rm -rf build && webpack --config webpack.config.prod.js && cp ./index.html build && cp ./jquery*.min.js build && cp -r i18n build && cp -r dhis2 build && cp -r extjs build && npm run manifest",
    "manifest": "d2-manifest package.json build/manifest.webapp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/pivot-tables-app.git"
  },
  "keywords": [
    "DHIS 2",
    "HISP",
    "Pivot"
  ],
  "author": "Jan Henrik Ã˜verland",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/pivot-tables-app/issues"
  },
  "homepage": "https://github.com/dhis2/pivot-tables-app#readme",
  "dependencies": {
    "d2-analysis": "32.0.1",
    "d2-utilizr": "0.2.16"
  },
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "2.1.0",
    "d2-manifest": "1.0.0",
    "eslint": "5.14.1",
    "eslint-config-dhis2": "3.0.7",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "img-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "node-sass": "4.11.0",
    "sass-loader": "7",
    "style-loader": "0.23.1",
    "susy": "3.0.5",
    "url-loader": "^1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1"
  },
  "manifest.webapp": {
    "name": "Pivot tables app",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "DHIS2 visualization team"
    },
    "activities": {
      "dhis": {
        "href": "..",
        "auth": "admin:district",
        "devHref": "http://localhost:8080"
      }
    }
  }
}
